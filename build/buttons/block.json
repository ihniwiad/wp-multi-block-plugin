{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "bsx-blocks/buttons",
  "version": "0.1.0",
  "title": "BSX Buttons",
  "category": "design",
  "allowedBlocks": [
    "bsx-blocks/button",
    "bsx-blocks/button-label"
  ],
  "description": "Single Button, multiple Buttons or text Label with Button or Link.",
  "example": {},
  "attributes": {
    "templateName": {
      "type": "string"
    },
    "textAlign": {
      "type": "string",
      "default": ""
    },
    "marginBefore": {
      "type": "string",
      "default": ""
    },
    "marginAfter": {
      "type": "string",
      "default": ""
    }
  },
  "supports": {
    "html": false,
    "className": false
  },
  "textdomain": "bsx-blocks",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css"
}