{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "bsx-blocks/check-list",
	"version": "0.1.0",
	"title": "BSX Check List",
	"category": "text",
	"allowedBlocks": [ "bsx-blocks/check-list-item" ],
	"description": "Create a check list.",
	"example": {},
    "attributes": {
        "values": {
            "type": "string",
            "source": "html",
            "selector": "ul",
            "multiline": "li",
            "__unstableMultilineWrapperTags": [ "ul" ],
            "default": "",
            "__experimentalRole": "content"
        },
        "state": {
            "type": "string",
            "default": "primary"
        },
        "marginLeft": {
            "type": "string"
        },
        "marginRight": {
            "type": "string"
        },
        "marginBefore": {
            "type": "string"
        },
        "marginAfter": {
            "type": "string"
        },
        "display": {
            "type": "string"
        },
        "textAlign": {
            "type": "string",
            "default": ""
        }
    },
	"supports": {
        "className": false,
		"__unstablePasteTextInline": true,
		"__experimentalSelector": "ul",
		"__experimentalSlashInserter": true,
        "__experimentalOnEnter": true
	},
	"textdomain": "bsx-blocks",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css"
}
